<!DOCTYPE html [<!ENTITY nbsp "&#160;"> <!ENTITY copy "&#169;">  <!ENTITY bull "&#8226;"> <!ENTITY lt "&#60;"><!ENTITY gt "&#62;"> ]>

<t:container xmlns="http://www.w3.org/1999/xhtml" xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">
	<div class="row">
		<article class="col-xs-12 col-sm-12 col-md-12 col-lg-12">
			<!-- Start results area -->
			Hi from customer implementation
			<t:if test="searchresults">
				<div class="jarviswidget jarviswidget-color-darken" id="wid-id-1" data-widget-colorbutton="false"
					data-widget-editbutton="false" data-widget-togglebutton="false" data-widget-deletebutton="false"
					data-widget-fullscreenbutton="false" data-widget-custombutton="false" data-widget-sortable="false">
					<header>
						<span class="widget-icon"> <i class="fa fa-table"></i>
						</span>
						<h2>Results</h2>
						<h2>[ Showing ${searchresults.results.size()} hits out of ${searchresults.total} ] for
							${searchValue} in ${searchTypeFilter}</h2>
						<t:graphene.ui.searchlegend />
					</header>
					<!-- widget div-->
					<div>

						<!-- widget content -->
						<div class="widget-body no-padding">
							<div class="widget-body-toolbar"></div>
							<t:jquery.datatable t:id="resultsdt" t:source="searchresults.results" t:model="model" t:row="currentSearchResult"
								t:options="options" class="table table-striped table-bordered table-hover table-responsive" t:rowsPerPage="20"
								t:empty="block:emptyResults">

								<p:rankCell>
									 ${rank}
								</p:rankCell>
								<p:actionsCell>
									<t:if test="reportPageLink">
										<t:pagelink class="btn btn-action" page="prop:reportPageLink" context="reportId" target="report-${reportId}"
											title="${score}">
											<i class="fa fa-folder-open-o" /> ${reportType} ${reportId}
												</t:pagelink>
										<br />
										<p:else>Report View Unavailable</p:else>
									</t:if>

									<a class="btn btn-action" href="${extLink}" target="graph-${reportId}"> <i class="fa fa-code-fork"> </i>
										Graph
									</a>
								</p:actionsCell>

								<p:informationIconsCell>
									<t:loop source="iconList" value="currentIcon">
										<span class="${currentIcon.first} label alert-info" title="${currentIcon.second}">&nbsp; </span>
									</t:loop>
								</p:informationIconsCell>
								<p:dateCell>
									${date}
								</p:dateCell>
								<p:amountCell>
									${formattedAmount}
								</p:amountCell>
								<p:subjectsCell>
									<t:loop source="nameList" value="currentName">
										<a class="btn btn-xs pivotlink" style="" href="${getNamePivotLink(currentName.third)}">
											${currentName.second} </a>
										<br />
										<br />
									</t:loop>
								</p:subjectsCell>
								<p:addressListCell>
									<t:loop source="addressList" value="currentAddress">
										<a class="btn btn-xs pivotlink" style="" href="${getAddressPivotLink(currentAddress.third)}">
											${currentAddress.second} </a>
										<br />
									</t:loop>
								</p:addressListCell>
								<p:communicationIdentifierListCell>
									<t:loop source="cIdentifierList" value="currentProperty">
										<a class="btn btn-xs pivotlink" style="${getStyleFor(currentProperty)}" href="${getPivotLink(currentProperty?.key)}">
											${currentProperty?.friendlyText}</a>
									</t:loop>
								</p:communicationIdentifierListCell>
								<p:identifierListCell>
									<t:loop source="identifierList" value="currentIdentifier">
										<a class="btn btn-xs pivotlink" style="" href="${getPivotLink(currentIdentifier.third)}">
											${currentIdentifier.second}</a>

									</t:loop>
								</p:identifierListCell>
							</t:jquery.datatable>

							<t:block t:id="emptyResults">
								<div id="noentitys">(No results found)</div>
							</t:block>

						</div>
						<!-- end widget content -->
					</div>
					<!-- end widget div -->
				</div>
			</t:if>
			<!-- end results area -->
		</article>
	</div>

</t:container>