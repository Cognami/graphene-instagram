<!DOCTYPE html [<!ENTITY nbsp "&#160;"> <!ENTITY copy "&#169;">  <!ENTITY bull "&#8226;"> <!ENTITY lt "&#60;"><!ENTITY gt "&#62;"> ]>

<t:container xmlns="http://www.w3.org/1999/xhtml"
	xmlns:t="http://tapestry.apache.org/schema/tapestry_5_3.xsd"
	xmlns:p="tapestry:parameter">

	<!-- Narrative -->
	<t:if test="listOfThings">

		<li class="col-md-12"><t:sectionTreeButton
				t:color="literal:ENTITY" t:typeName="literal:Subjects"
				t:typeCount="listOfThings.size()" />
			<ul>
				<t:loop source="listOfThings" value="currentThing"
					t:index="index">
					<li><t:entityTreeButton t:id="subjectTreeButton"
							t:color="literal:ENTITY" />
						<ul>
							<li><div class="row">
									<div class="col-md-6">
										<h3>Details</h3>
										<div t:type="graphene/ui/meaningfulbeandisplay"
											object="currentThing" reorder=""
											exclude="subjectAddress,subjectId,subjectPhone,subjectAlternateName,subjectEmail" /></div>

										
									</div></li>
						</ul></li>
				</t:loop>
			</ul></li>
	</t:if>

</t:container>